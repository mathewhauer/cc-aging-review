@article{musitu-ferrer_relationships_2019,
	title = {Relationships between {Parental} {Socialization} {Styles}, {Empathy} and {Connectedness} with {Nature}: {Their} {Implications} in {Environmentalism}},
	volume = {16},
	issn = {1660-4601},
	shorttitle = {Relationships between {Parental} {Socialization} {Styles}, {Empathy} and {Connectedness} with {Nature}},
