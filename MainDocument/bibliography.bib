	urldate = {2022-09-08},
	journal = {Journal of the American Heart Association},
	author = {Wettstein, Zachary S. and Hoshiko, Sumi and Fahimi, Jahan and Harrison, Robert J. and Cascio, Wayne E. and Rappold, Ana G.},
	month = apr,
